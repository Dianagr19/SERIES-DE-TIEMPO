##########################     MATRICES    #####################################


matriz1 <- matrix(c(2,4,20,40,200,400,2000,-2,-4,-20,-40,-200,-400,-2000,-4000),4,4)
matriz1
matriz1[4,1]
matriz1[,c(2,3)] #solo mantiene la columna 2 y 3
matriz1 %*% matriz1 #operaciÃ³n de matriz
matriz1 * matriz1 
#combinar matrices
?cbind #
?rbind  #combinar matricez a travez de las columnas

matriz2 <-matrix(c(1,2,3,4,5,6,7),2,4) #matriz on estos datos de dos filas y 4 columnas
matriz2

matriz3 <- cbind(c(10,20,30,50), matriz2[,4])# combina esta nueva matriz con los datos de la matriz2 columna 4
matriz3

matriz4 <- matrix (c(8,7,6,5,4,3,2,1),4,2)
matriz4
matriz5 <- rbind(matriz1[2,],matriz4[2,])
matriz5
